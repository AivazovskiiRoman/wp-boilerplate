{
  "name": "inf_theme",
  "version": "1.0.0",
  "author": "Team Eightshift",
  "private": true,
  "main": "",
  "scripts": {
    "__eslintTheme": "eslint skin/assets/**/*.js",
    "__stylelintTheme": "stylelint skin/assets/**/*.scss",
    "lintStyle": "npm run __stylelintTheme",
    "lintJs": "npm run __eslintTheme",
    "lint": "npm run lintJs && npm run lintStyle && bash bin/lintPhp.sh",
    "precommit": "npm run lint",
    "rename": "node bin/rename-runnable.js",
    "start": "webpack --progress --watch --display-error-details --display-reasons --mode development",
    "build": "NODE_ENV=production webpack --progress --mode production",
    "setup": "npm install && node bin/setup.js",
    "setup-wp": "node bin/setup-wp.js"
  },
  "devDependencies": {
    "chalk": "^2.4.1",
    "generate-password": "^1.4.1",
    "node-emoji": "^1.8.1",
    "ora": "^3.0.0",
    "promisify-child-process": "^2.1.2",
    "prompt-sync": "^4.1.6",
    "replace-in-file": "^3.4.2"
  },
  "dependencies": {},
  "browserslist": [
    "android >= 4.2",
    "not ie < 11",
    "last 2 versions",
    "Safari >= 8"
  ]
}
