{
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "*",
    "jakub-onderka/php-console-highlighter": "*",
    "jakub-onderka/php-parallel-lint": "*",
    "infinum/coding-standards-wp": "*"
  },
  "scripts": {
    "post-install-cmd": [
      "\"vendor/bin/phpcs\" --config-set installed_paths vendor/infinum/coding-standards-wp"
    ],
    "post-update-cmd": [
      "\"vendor/bin/phpcs\" --config-set installed_paths vendor/infinum/coding-standards-wp"
    ]
  },
  "autoload": {
    "classmap": [
      "wp-content/themes/init_theme_name/",
      "wp-content/themes/init_theme_name/admin/",
      "wp-content/themes/init_theme_name/helpers/",
      "wp-content/themes/init_theme_name/includes/",
      "wp-content/themes/init_theme_name/plugins/",
      "wp-content/themes/init_theme_name/plugins/acf/",
      "wp-content/themes/init_theme_name/theme/",
      "wp-content/themes/init_theme_name/theme/menu/",
      "wp-content/themes/init_theme_name/theme/utils/"
    ]
  }
}
